{
  "name": "angular2-authentication-sample",
  "version": "0.0.1",
  "description": "This is a sample that shows how to add authentication to an Angular 2 (ng2) app",
  "main": "",
  "scripts": {
    "start": "tsc && concurrently \"npm run tsc:w\" \"npm run lite\" ",
    "tsc": "tsc",
    "tsc:w": "tsc -w",
    "lite": "lite-server",
    "typings": "typings",
    "docker-build": "docker build -t ng2-quickstart .",
    "docker": "npm run docker-build && docker run -it --rm -p 3000:3000 -p 3001:3001 ng2-quickstart",
    "postinstall": "typings install"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/auth0/angular2-authentication-sample.git"
  },
  "contributors": [
    "Martin Gontovnikas (http://gon.to) <martin@gon.to>",
    "PatrickJS <github@gdi2290.com>",
    "Ryan Chenkie <ryanchenkie@gmail.com>"
  ],
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/auth0/angular2-authentication-sample/issues"
  },
  "homepage": "https://github.com/auth0/angular2-authentication-sample",
  "dependencies": {
    "typings": "0.7.12",
    "angular2": "2.0.0-beta.14",
    "angular2-jwt": "0.1.9",
    "jwt-decode": "^1.5.1",
    "systemjs": "0.19.25",
    "rxjs": "5.0.0-beta.2",
    "zone.js": "0.6.6",
    "body-parser": "^1.15.0",
    "compression": "^1.6.1",
    "cors": "^2.7.1",
    "express": "^4.13.4",
    "ionic-framework": "2.0.0-beta.1",
    "ionicons": "3.0.0-alpha.3",
    "reflect-metadata": "0.1.2",
    "bootstrap": "^3.3.6",
    "es6-promise": "^3.1.2",
    "es6-shim": "^0.35.0",
    "es7-reflect-metadata": "^1.6.0"
  },
  "devDependencies": {
    "concurrently": "^2.0.0",
    "lite-server": "^2.2.0",
    "typescript": "^1.8.9",
    "typings":"^0.7.12",
    "babel-core": "6.5.2",
    "babel-loader": "6.2.3",
    "babel-plugin-transform-decorators-legacy": "1.3.4",
    "babel-preset-es2015": "6.5.0",
    "strip-sourcemap-loader": "0.0.1",
