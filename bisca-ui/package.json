{
  "name": "bisca-ui",
  "version": "0.0.1",
  "description": "This is the game BiscaJE",
  "main": "",
  "scripts": {
    "start": "npm install && webpack install && tsc && concurrently \"tsc -w\" \"lite-server\"",
    "docker-build": "docker build -t ng2-quickstart .",
    "docker": "npm run docker-build && docker run -it --rm -p 3000:3000 -p 3001:3001 ng2-quickstart",
    "e2e": "tsc && concurrently \"http-server\" \"protractor protractor.config.js\"",
    "lite": "tsc && webpack install && lite-server",
    "all": "npm install && webpack install && tsc && lite-server",
    "notsclite": "lite-server",
    "postinstall": "typings install",
    "test": "tsc && concurrently \"tsc -w\" \"karma start karma.conf.js\"",
    "tsc": "tsc",
    "tsc:w": "tsc -w",
    "typings": "typings",
    "webdriver:update": "webdriver-manager update",
    "front": "webpack-dev-server --inline -d --colors --display-error-details --display-cached --port 3000"
  },
  "repository": {
