    "tsconfig-lint": "^0.7.0",
    "tslint": "^3.5.0",
    "tslint-loader": "^2.1.3",
    "typedoc": "^0.3.12",
    "url-loader": "^0.5.7",
    "webpack": "^1.12.14",
    "webpack-dev-server": "^1.14.1"
  }
}
